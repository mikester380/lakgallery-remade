{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "semi": ["error", "always"],
    "quotes": [0, "double"],
    "react/prop-types": 0,
    "import/no-named-as-default": 0,
    "jsx-a11y/anchor-is-valid": 0
  },
  "plugins": ["react", "import", "jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
